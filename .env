# Copy this file to .env and fill in your secrets
AI_PROVIDER=openai         # openai | anthropic | local
AI_API_KEY=your_ai_api_key

OPENAI_MODEL=gpt-4o-mini
ANTHROPIC_MODEL=claude-3-5-sonnet-20240620

PRIVATE_KEY=0xYOUR_PRIVATE_KEY
PUBLIC_ADDRESS=0xYourPublicAddress
CHAIN_ID=1
RPC_HTTP_URL=https://mainnet.infura.io/v3/YOUR_KEY
RPC_WS_URL=wss://mainnet.infura.io/ws/v3/YOUR_KEY

# Optional
FLASHBOTS_RELAY_URL=https://relay.flashbots.net
MEV_PROTECT=false
DRY_RUN=true
GAS_PRIORITY_GWEI=2
MAX_FEE_MULTIPLIER=1.2
LOG_LEVEL=INFO
